{
    "name": "sirone/Chatwork",
    "description": "Chatwork plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Chatwork\\": "./plugins/Chatwork/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chatwork\\Test\\": "./plugins/Chatwork/tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
